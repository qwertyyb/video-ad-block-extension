{
    "name": "视频广告屏蔽",
    "version": "3.0.6",
    "manifest_version": 2,
    "description": "屏蔽国内各大视频网站的广告",
    "browser_action": {
        "default_icon": "icons/off.png",
        "default_title": "视频广告屏蔽",
        "default_badge": "1"
    },
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "page": "background.html"
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "proxy",
        "webRequest",
        "webRequestBlocking"
    ],
    "update_url": "http://7xv7aw.com1.z0.glb.clouddn.com/adblock/version.xml"
}
