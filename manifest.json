{
	"name": "视频广告屏蔽",
	"version": "20171201",
	"manifest_version": 2,
	"description": "屏蔽国内各大视频网站的广告",
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"48": "icons/icon48.png",
		"64": "icons/icon64.png",
		"128": "icons/icon128.png"
	},
	"background": {
		"scripts": ["scripts/cleanVideoAd.js"]
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"proxy",
		"webRequest",
		"webRequestBlocking"
	]
}